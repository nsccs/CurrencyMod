{
    "schemaVersion": 1,
    "id": "currencymod",
    "version": "${version}",
    "name": "Currency Mod",
    "description": "A mod that adds currency to the game.",
    "authors": [
        "Me!"
    ],
    "contact": {
        "homepage": "https://fabricmc.net/",
        "sources": "https://github.com/FabricMC/fabric-example-mod"
    },
    "license": "CC0-1.0",
    "icon": "assets/currencymod/icon.png",
